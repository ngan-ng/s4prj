+import {ADMIN_ACTION_TYPES} from "./admin.types";
+
+const ADMIN_INITIAL_STATE = {
+    currentAdmin: {
+        token: "",
+        requestToken: "",
+        userName: "",
+        roles: [],
+    },
+    isLoading: false,
+    error: null,
+};
+
+export const adminReducer = (state = ADMIN_INITIAL_STATE, action) => {
+    const { type, payload } = action;
+
+    switch (type) {
+        case ADMIN_ACTION_TYPES.SIGN_IN_SUCCESS:
+            return {
+                ...state,
+                currentAdmin: payload,
+            };
+        case ADMIN_ACTION_TYPES.SIGN_OUT_SUCCESS:
+            return {
+                ...state,
+                currentMember: null,
+            };
+        case ADMIN_ACTION_TYPES.SIGN_OUT_FAILED:
+        case ADMIN_ACTION_TYPES.SIGN_IN_FAILED:
+        case ADMIN_ACTION_TYPES.SIGN_UP_FAILED:
+            return {
+                ...state,
+                error: payload,
+            };
+        default:
+            return state;
+    }
+};
